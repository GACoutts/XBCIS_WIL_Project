{
  "name": "xbcis-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "npm --prefix frontend run dev",
    "build:frontend": "npm --prefix frontend run build",
    "preview:frontend": "npm --prefix frontend run preview",
    "dev:backend": "npm --prefix backend run dev",
    "dev:both": "concurrently -k -n FE,BE -c green,blue \"npm:dev:frontend\" \"npm:dev:backend\""
  },
  "devDependencies": {
        "concurrently": "^9.2.0"
  }
}
